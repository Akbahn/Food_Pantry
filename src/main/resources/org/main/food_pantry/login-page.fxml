<AnchorPane prefHeight="532.0" prefWidth="778.0" stylesheets="@Styles/splashpagestyle.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.main.food_pantry.Controllers.LoginPageController">
   <children>
      <VBox alignment="CENTER_LEFT" layoutX="103.0" layoutY="198.0" prefHeight="190.0" prefWidth="573.0">
         <children>
            <Label text="Username:" />
            <TextField fx:id="usernameTxtF" prefHeight="26.0" prefWidth="384.0" />
            <Label text="Password:" />
            <TextField fx:id="passwordTxtF" />
            <Button fx:id="signInBtn" mnemonicParsing="false" onAction="#goToPage" prefHeight="26.0" prefWidth="574.0" text="Sign In" />
            <Hyperlink fx:id="forgotPasswordBtn" onAction="#goChangePasswordPage" text="Forgot Password?" />
         </children>
      </VBox>
      <Button fx:id="backBtn" layoutX="48.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBackPage" text="Back" />
      <Label layoutX="343.0" layoutY="126.0" text="Sign In">
         <font>
            <Font size="33.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
